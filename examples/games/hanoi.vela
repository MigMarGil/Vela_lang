func hanoi_moves(n: int) -> int {
    if n == 1 {
        return 1
    }
    return 2 * hanoi_moves(n - 1) + 1
}

func hanoi_iterative(n: int) -> int {
    auto result = 1
    auto i = 1
    while i < n {
        result = result * 2 + 1
        i = i + 1
    }
    return result
}

func main() -> void {
    auto i = 1
    while i <= 10 {
        print(hanoi_moves(i))
        i = i + 1
    }
}
