func bubble_sort_iterations(n: int) -> int {
    auto swaps = 0
    auto i = 0
    
    while i < n - 1 {
        auto j = 0
        while j < n - i - 1 {
            if j > j + 1 {
                swaps = swaps + 1
            }
            j = j + 1
        }
        i = i + 1
    }
    
    return swaps
}

func main() -> void {
    print(bubble_sort_iterations(10))
}
