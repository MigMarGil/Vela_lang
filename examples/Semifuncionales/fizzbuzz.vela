func fizzbuzz(n: int) -> void {
    auto i = 1
    while i <= n {
        auto div3 = i % 3
        auto div5 = i % 5
        
        if div3 == 0 {
            if div5 == 0 {
                print(0)
            } else {
                print(3)
            }
        } else {
            if div5 == 0 {
                print(5)
            } else {
                print(i)
            }
        }
        
        i = i + 1
    }
}

func main() -> void {
    fizzbuzz(100)
}
