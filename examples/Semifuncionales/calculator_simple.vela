func add(a: int, b: int) -> int {
    return a + b
}

func subtract(a: int, b: int) -> int {
    return a - b
}

func multiply(a: int, b: int) -> int {
    return a * b
}

func divide(a: int, b: int) -> int {
    if b == 0 {
        return 0
    }
    return a / b
}

func main() -> void {
    print(add(98123098138, 5))
    print(subtract(10, 5))
    print(multiply(10, 5))
    print(divide(10, 0))
}
