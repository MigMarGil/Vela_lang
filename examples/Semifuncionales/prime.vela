func is_prime(n: int) -> int {
    if n <= 1 {
        return 0
    }
    
    if n == 2 {
        return 1
    }
    
    auto i = 2
    while i * i <= n {
        if n % i == 0 {
            return 0
        }
        i = i + 1
    }
    
    return 1
}

func main() -> void {
    auto n = 2
    while n <= 20 {
        if is_prime(n) == 1 {
            print(n)
        }
        n = n + 1
    }
}
