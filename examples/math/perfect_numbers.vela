func sum_divisors(n: int) -> int {
    auto sum = 0
    auto i = 1
    
    while i < n {
        if n % i == 0 {
            sum = sum + i
        }
        i = i + 1
    }
    
    return sum
}

func is_perfect(n: int) -> int {
    if sum_divisors(n) == n {
        return 1
    }
    return 0
}

func main() -> void {
    auto n = 1
    while n <= 10000 {
        if is_perfect(n) == 1 {
            print(n)
        }
        n = n + 1
    }
}
